@model BackMeow.Models.ViewModel.MemberViewModel
@{
    ViewBag.Title = "Member";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Member</h2>
@using (Html.BeginForm("Member", "System", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    <input type="text" value="@ViewBag.TESTCityDDL" id="TESTCityDDL">
    <div class="control-group">
        <label class="control-label" for="CityDDL">縣市</label>
        <div class="controls">
            <select id="CityDDL"></select>
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="CountyDDL">鄉鎮市區</label>
        <div class="controls">
            <select id="CountyDDL"></select>
        </div>
    </div>
}
@section Scripts {
    @Html.Partial("_TaiwanZipCode")
}